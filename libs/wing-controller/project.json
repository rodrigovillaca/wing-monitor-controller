{
  "name": "wing-controller",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "root": "libs/wing-controller",
  "sourceRoot": "libs/wing-controller/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/wing-controller/jest.config.ts"
      }
    },
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/wing-controller",
        "main": "libs/wing-controller/src/index.ts",
        "tsConfig": "libs/wing-controller/tsconfig.lib.json",
        "assets": ["libs/wing-controller/*.md"]
      }
    }
  }
}
